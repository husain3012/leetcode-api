{
  "name": "leetcode-public-api",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Husain Shahid Rao",
  "main": "dist/index.js",
  "module": "dist/leetcode-public-api.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/husain3012/leetcode-public-api",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=17"
  },
  "size-limit": [
    {
      "path": "dist/leetcode-public-api.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/leetcode-public-api.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {},
  "keywords": [
    "leetcode",
    "leetcode-api",
    "scraper"
  ]
}
